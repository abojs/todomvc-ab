{
  "name": "todomvc-ab",
  "src": "./dist",
  "serverHost": "localhost:9595",
  "envs": {
    "dev": {
      "serverHost": "localhost:9595",
      "cmds": [
        ["rm", "-rf", "dist"],
        ["foso", "build", "-m", "--host", "localhost:9595", "--env", "dev"]
      ]
    },
    "prod": {
      "serverHost": "localhost:9595",
      "cmds": [
        ["rm", "-rf", "dist"],
        ["foso", "build", "-m", "--host", "localhost:9595/prod/dtm", "--env", "prod"]
      ]
    }
  }
}
